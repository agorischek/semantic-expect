{
  "name": "semantic-expect",
  "version": "0.0.6",
  "description": "LLM-based test assertions",
  "repository": {
    "type": "git",
    "url": "https://github.com/agorischek/semantic-expect"
  },
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "badge": "badge-roll affix",
    "build": "tsc --project tsconfig.build.json",
    "dev:jest": "jest --config dev/jest/jest.config.js",
    "dev:unformatted": "vitest --config dev/unformatted/vitest.dev.config.ts",
    "dev:vitest": "vitest --config dev/vitest/vitest.dev.config.ts",
    "start": "npm t dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:llm": "vitest run --config vitest.llm.config.ts"
  },
  "keywords": [
    "ai",
    "assert",
    "assertion",
    "expect",
    "jest",
    "llm",
    "openai",
    "tests",
    "unit",
    "vitest"
  ],
  "author": "Alex Gorischek",
  "license": "MIT",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@vitest/coverage-v8": "^0.34.5",
    "badge-roll": "^0.0.6",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "radash": "^11.0.0",
    "strip-ansi": "^7.1.0",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "jest-matcher-utils": "^29.7.0",
    "openai": "^4.10.0"
  }
}
